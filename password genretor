import random;
lower="abcdefghijklmnopqrstuvwxyz";
upper="ABCDEFGHIJKLMNOPQRSTUVWXYZ";
no= "0123456789";

symbol="!@#$%^&*+-_=~?/.,|}{{}:;<>`";
stri=lower+upper+no+symbol;
leng=70;
password="".join(random.sample(stri,leng));
print("your new password is : "+password)


